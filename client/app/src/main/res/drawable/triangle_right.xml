<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate
            android:fromDegrees="45"
            android:pivotX="-18%"
            android:pivotY="-50%">
            <shape android:shape="rectangle">
                <size
                    android:width="20dp"
                    android:height="20dp" />
                <corners android:radius="2dp"/>
                <solid android:color="#FDFAE0" />
            </shape>
        </rotate>
    </item>
</layer-list>
